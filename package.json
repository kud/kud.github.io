{
  "name": "_kud.profile",
  "version": "2.0.4",
  "author": "Erwann Mest",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kud/kud.github.io.git"
  },
  "scripts": {
    "prettier": "prettier --trailing-comma all --no-semi --write \"**/*.{js,css}\" \"!dist/**/*\"",
    "start": "webpack-dev-server --host 0.0.0.0 --watch --config=./webpack.babel.js",
    "build": "rm -rf ./dist/ && webpack --progress --colors --config=./webpack.babel.js",
    "build:min": "rm -rf ./dist/ && webpack --progress --colors --config=./webpack.min.babel.js",
    "deploy": "npm run build:min && gh-pages -d dist -b master"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "cname-webpack-plugin": "1.0.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "gh-pages": "1.0.0",
    "html-webpack-plugin": "2.30.1",
    "image-webpack-loader": "3.4.2",
    "nunjucks": "3.0.1",
    "nunjucks-loader": "2.4.8",
    "prettier": "1.8.2",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "dependencies": {
    "normalize.css": "7.0.0"
  }
}
